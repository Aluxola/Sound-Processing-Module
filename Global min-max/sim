v000001a1cd326bd0_0 .var "sample_valid", 0 0;
v000001a1cd3269f0_0 .var/i "status", 31 0;
E_000001a1cd289c00 .event posedge, v000001a1cd2a2e90_0;
S_000001a1cd2c6690 .scope module, "uut" "min_max_amplitude" 2 18, 3 3 0, S_000001a1cd28bb90;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 16 "sample_in";
    .port_info 3 /INPUT 1 "sample_valid";
    .port_info 4 /OUTPUT 16 "min_out";
    .port_info 5 /OUTPUT 16 "max_out";
v000001a1cd2a2e90_0 .net "clk", 0 0, v000001a1cd2a27f0_0;  1 drivers
v000001a1cd28b0f0_0 .var/s "max_out", 15 0;
v000001a1cd28bd20_0 .var/s "min_out", 15 0;
v000001a1cd2c6820_0 .net "rst", 0 0, v000001a1cd2a2a70_0;  1 drivers
v000001a1cd2c68c0_0 .net/s "sample_in", 15 0, v000001a1cd2d3870_0;  1 drivers
v000001a1cd2c6960_0 .net "sample_valid", 0 0, v000001a1cd326bd0_0;  1 drivers
E_000001a1cd289540 .event posedge, v000001a1cd2c6820_0, v000001a1cd2a2e90_0;
    .scope S_000001a1cd2c6690;
T_0 ;
    %wait E_000001a1cd289540;
    %load/vec4 v000001a1cd2c6820_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 32767, 0, 16;
    %assign/vec4 v000001a1cd28bd20_0, 0;
    %pushi/vec4 32768, 0, 16;
    %assign/vec4 v000001a1cd28b0f0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v000001a1cd2c6960_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v000001a1cd2c68c0_0;
    %load/vec4 v000001a1cd28bd20_0;
    %cmp/s;
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v000001a1cd2c68c0_0;
    %assign/vec4 v000001a1cd28bd20_0, 0;
T_0.4 ;
    %load/vec4 v000001a1cd28b0f0_0;
    %load/vec4 v000001a1cd2c68c0_0;
    %cmp/s;
    %jmp/0xz  T_0.6, 5;
    %load/vec4 v000001a1cd2c68c0_0;
    %assign/vec4 v000001a1cd28b0f0_0, 0;
T_0.6 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_000001a1cd28bb90;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "simulation.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars", 32'sb00000000000000000000000000000000, S_000001a1cd28bb90 {0 0 0};
    %end;
    .thread T_1;
    .scope S_000001a1cd28bb90;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a1cd2a27f0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_000001a1cd28bb90;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v000001a1cd2a27f0_0;
    %inv;
    %store/vec4 v000001a1cd2a27f0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_000001a1cd28bb90;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001a1cd2a2a70_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a1cd326bd0_0, 0, 1;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v000001a1cd2d3870_0, 0, 16;
    %vpi_func 2 42 "$fopen" 32, "audio_samples.txt", "r" {0 0 0};
    %store/vec4 v000001a1cd2a2890_0, 0, 32;
    %load/vec4 v000001a1cd2a2890_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_4.0, 4;
    %vpi_call 2 45 "$display", "ERROR: Failed to open audio_samples.txt" {0 0 0};
    %vpi_call 2 46 "$finish" {0 0 0};
    %jmp T_4.1;
T_4.0 ;
    %vpi_call 2 48 "$display", "Reading samples from audio_samples.txt..." {0 0 0};
T_4.1 ;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a1cd2a2a70_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v000001a1cd2a2bb0_0, 0, 32;
T_4.2 ;
    %vpi_func 2 56 "$feof" 32, v000001a1cd2a2890_0 {0 0 0};
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz T_4.3, 8;
    %vpi_func 2 57 "$fscanf" 32, v000001a1cd2a2890_0, "%d\012", v000001a1cd2a2b10_0 {0 0 0};
    %store/vec4 v000001a1cd3269f0_0, 0, 32;
    %load/vec4 v000001a1cd3269f0_0;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_4.4, 4;
    %wait E_000001a1cd289c00;
    %load/vec4 v000001a1cd2a2b10_0;
    %store/vec4 v000001a1cd2d3870_0, 0, 16;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v000001a1cd326bd0_0, 0, 1;
    %load/vec4 v000001a1cd2a2bb0_0;
    %addi 1, 0, 32;
    %store/vec4 v000001a1cd2a2bb0_0, 0, 32;
T_4.4 ;
    %jmp T_4.2;
T_4.3 ;
    %wait E_000001a1cd289c00;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v000001a1cd326bd0_0, 0, 1;
    %vpi_call 2 69 "$fclose", v000001a1cd2a2890_0 {0 0 0};
    %delay 50000, 0;
    %vpi_call 2 73 "$display", "== Results ==" {0 0 0};
    %vpi_call 2 74 "$display", "Samples processed: %0d", v000001a1cd2a2bb0_0 {0 0 0};
    %vpi_call 2 75 "$display", "Minimum Amplitude = %d", v000001a1cd2a29d0_0 {0 0 0};
    %vpi_call 2 76 "$display", "Maximum Amplitude = %d", v000001a1cd2a2930_0 {0 0 0};
    %vpi_call 2 78 "$finish" {0 0 0};
    %end;
    .thread T_4;
